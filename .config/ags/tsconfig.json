{
	"compilerOptions": {
		"target": "ES2022",
		"module": "ES2022",
		"lib": ["ES2022"],
		"allowJs": true,
		"checkJs": true,
		"strict": true,
		"noImplicitAny": false,
		"baseUrl": "src",
		"paths": {
			"@services/*": ["services/*"],
			"@lib/*": ["lib/*"],
			"@widgets/*": ["widgets/*"],
			"@bar/*": ["bar/*"],
			"@osd/*": ["osd/*"]
		},
		"typeRoots": ["./types"],
		"skipLibCheck": true
	}
}
