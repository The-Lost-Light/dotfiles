{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"lib": ["ESNext"],
		"strict": true,
		"noImplicitAny": false,
		"baseUrl": "src",
		"paths": {
			"@services/*": ["services/*"],
			"@lib/*": ["lib/*"],
			"@widgets/*": ["widgets/*"],
			"@bar/*": ["bar/*"],
			"@osd/*": ["osd/*"]
		},
		"typeRoots": ["./types"],
		"skipLibCheck": true
	}
}
